  "name": "customer_transaction_app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "concurrently \"gh-pages -d dist\" \"npm run server\"",
    "start": "concurrently \"npm run server\" \"npm run dev\"",
